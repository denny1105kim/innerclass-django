##아래 내용 참조해서 .env를 root-directory에 생성바랍니다.

# -----------------------------------------------------------------------------
# Django core
# -----------------------------------------------------------------------------
DJANGO_SECRET_KEY=your_secret_key_here
DJANGO_DEBUG=True
DJANGO_ALLOWED_HOSTS=127.0.0.1,localhost

# -----------------------------------------------------------------------------
# PostgreSQL
# -----------------------------------------------------------------------------
POSTGRES_DB=innerclass_db
POSTGRES_USER=admin
POSTGRES_PASSWORD=your_db_password_here
POSTGRES_HOST=localhost
POSTGRES_PORT=5432

# -----------------------------------------------------------------------------
# SocialLogin
# -----------------------------------------------------------------------------
# 구글 클라우드 콘솔 키 (팀장에게 문의)
SOCIAL_AUTH_GOOGLE_CLIENT_ID=your_google_client_id_here
SOCIAL_AUTH_GOOGLE_SECRET=your_google_secret_here
STATE_SECRET=random_string_security_key_1234

# 개발환경: localhost / 배포환경: 도메인
GOOGLE_REDIRECT_URI=http://localhost:5173/oauth/callback/google